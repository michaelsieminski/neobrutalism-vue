{
	"$schema": "https://shadcn-vue.com/schema/registry.json",
	"name": "neobrutalism-vue",
	"homepage": "https://neobrutalism-vue.vercel.app",
	"items": [
		{
			"name": "accordion",
			"type": "registry:ui",
			"title": "Accordion",
			"description": "A vertically stacked set of interactive headings that each reveal a section of content.",
			"dependencies": ["reka-ui", "@vueuse/core"],
			"registryDependencies": ["utils"],
			"files": [
				{
					"path": "registry/neobrutalism/ui/accordion/Accordion.vue",
					"type": "registry:component"
				},
				{
					"path": "registry/neobrutalism/ui/accordion/AccordionContent.vue",
					"type": "registry:component"
				},
				{
					"path": "registry/neobrutalism/ui/accordion/AccordionItem.vue",
					"type": "registry:component"
				},
				{
					"path": "registry/neobrutalism/ui/accordion/AccordionTrigger.vue",
					"type": "registry:component"
				},
				{
					"path": "registry/neobrutalism/ui/accordion/index.ts",
					"type": "registry:component"
				}
			],
			"tailwind": {
				"config": {
					"theme": {
						"extend": {
							"keyframes": {
								"accordion-down": {
									"from": {
										"height": "0"
									},
									"to": {
										"height": "var(--reka-accordion-content-height)"
									}
								},
								"accordion-up": {
									"from": {
										"height": "var(--reka-accordion-content-height)"
									},
									"to": {
										"height": "0"
									}
								}
							},
							"animation": {
								"accordion-down": "accordion-down 0.2s ease-out",
								"accordion-up": "accordion-up 0.2s ease-out"
							}
						}
					}
				}
			}
		},
		{
			"name": "utils",
			"type": "registry:lib",
			"files": [
				{
					"path": "lib/utils.ts",
					"type": "registry:lib"
				}
			]
		}
	]
}
