{
  "name": "neobrutalism-vue",
  "version": "0.0.1",
  "description": "A vue-based registry of neobrutalism-styled Tailwind components.",
  "author": "Michael Sieminski",
  "type": "module",
  "main": "./dist/neobrutalism-vue.cjs.js",
  "module": "./dist/neobrutalism-vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/neobrutalism-vue.es.js",
      "require": "./dist/neobrutalism-vue.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/vue-table": "8.21.3",
    "@vueuse/core": "13.9.0",
    "clsx": "2.1.1",
    "lucide-vue-next": "0.544.0",
    "reka-ui": "2.5.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.13",
    "vue": "3.5.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "6.0.1",
    "typescript": "5.9.3",
    "vite": "7.1.7",
    "vue-tsc": "3.1.0"
  }
}
