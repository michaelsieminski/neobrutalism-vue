{
	"installCommand": "bun install",
	"buildCommand": "bun run build",
	"headers": [
		{
			"source": "/r/(.*).json",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, OPTIONS"
				},
				{
					"key": "Content-Type",
					"value": "application/json"
				}
			]
		}
	]
}
